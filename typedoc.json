{
  "$schema": "https://typedoc.org/schema.json",
  "name": "RevGrid",
  "tsconfig": "./src/code/tsconfig.json",
  "entryPoints": ["./src/code"],
  "entryPointStrategy": "expand",
  "exclude": ["./src/code/**/index.ts"],
  "alwaysCreateEntryPointModule": false,
  "excludeInternal": true,
  "readme": "README.md",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "router": "structure-dir",
  "disableSources": true,
  "projectDocuments": [
    "src-docs/overview/index.md",
    "src-docs/architecture/index.md",
    "src-docs/guides/index.md",
    "src-docs/examples/index.md"
  ],
  "out": "./docs",
  "hostedBaseUrl": "https://revgrid.github.io/revgrid/",
  "navigationLinks": {
      "GitHub": "https://github.com/revgrid/revgrid",
      "NPM": "https://www.npmjs.com/package/revgrid",
      "Sponsors": "https://revgrid.github.io/revgrid/Overview/Sponsors/",
  },
}
